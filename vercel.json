{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/admin",
      "dest": "/truthos-console/index.html"
    },
    {
      "src": "/console", 
      "dest": "/truthos-console/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/watchman-world-news/index.html"
    }
  ]
}