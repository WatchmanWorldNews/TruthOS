{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install",
  "outputDirectory": "./",
  "functions": {
    "watchman-world-news/server.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/watchman-world-news/server.ts"
    },
    {
      "source": "/admin",
      "destination": "/truthos-console/index.html"
    },
    {
      "source": "/console",
      "destination": "/truthos-console/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/watchman-world-news/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}